{
  "use": ["postcss-import", "postcss-nested", "postcss-cssnext"],
  "input": "assets/styles/index.css",
  "output": ".tmp/build/stylesheets/main.css",
  "postcss-cssnext": {
    "browsers": [
      "> 1% in AU",
      "last 2 versions",
      "Firefox ESR",
      "ie >= 9",
      "iOS >= 8.4",
      "Safari >= 8",
      "Android >= 4.4"
    ]
  },
  "cssnano": {
    "autoprefixer": false
  }
}
